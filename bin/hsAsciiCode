map={}
str.each_line(){|line|
	array=line.split("\t")
	decimal=array[1].to_i
	hex=array[2].to_i
	character=array[0]
	map[decimal]=character
}

for raw in 0..15 do
	for i in 0..7 do
		if(i==0)then
			print " "
		end
		print "#{"%3s"%map[raw+i*16]} #{"%03d"%(raw+i*16)} #{"%02x"%(raw+i*16)}"
		if(i!=7)then
			print "   "
		end

	end
	puts ""
end